<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.speedtap">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_green_bubble_image"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SinglePlayerActivity"></activity>
        <activity android:name=".TwoPlayersActivity"></activity>
        <activity android:name=".ScoreList"></activity>
    </application>

</manifest>